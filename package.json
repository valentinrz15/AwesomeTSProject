{
  "name": "awesometsproject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "cp ./.env.dev ./.env; react-native run-android --variant=devDebug --appIdSuffix dev",
    "android:preprod": "cp ./.env.preprod ./.env ; react-native run-android --variant=preprodDebug --appIdSuffix preprod",
    "android:prod": "cp ./.env.prod ./.env ; react-native run-android --variant=prodDebug --appIdSuffix prod",
    "android:prod-release": "cp ./.env.prod ./.env ;cd android && ./gradlew clean && ./gradlew assembleProdRelease",
    "android:clean": "cd android; ./gradlew clean",
    "ios": "cp ./.env.dev ./.env ; react-native run-ios --scheme AwesomeTSProjectDev --configuration DebugDev --simulator 'iPhone 8'",
    "ios:preprod": "cp ./.env.preprod ./.env ;react-native run-ios --scheme AwesomeTSProjectDev --configuration DebugPreprod --simulator 'iPhone 8'",
    "ios:prod": "cp ./.env.prod ./.env ;react-native run-ios --scheme AwesomeTSProjectDev --configuration DebugProd --simulator 'iPhone 8'",
    "ios:clean": "rm -rf node_modules; yarn ; cd ios && rm -rf Pods && rm -fr DerivedData && pod cache clean --all && rm -fr build && pod install --repo-update && cd ..",
    "pod:install": "cd ios ; pod install ; cd ..",
    "android-apk:dev": "cp ./.env.dev ./.env ; cd android && ./gradlew clean && ENVFILE=.env.dev && ./gradlew assembleDevRelease ; open ./app/build/outputs/apk/dev/release",
    "android-apk:preprod": "cp ./.env.preprod ./.env ; cd android && ./gradlew clean && ENVFILE=.env.preprod && ./gradlew assemblePreprodRelease ; open ./app/build/outputs/apk/preprod/release",
    "android-apk:prod": "cp ./.env.prod ./.env ; cd android && ./gradlew clean && ENVFILE=.env.prod && ./gradlew assembleProdRelease ; open ./app/build/outputs/apk/prod/release",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.6",
    "@react-native-masked-view/masked-view": "0.2.6",
    "@react-native-picker/picker": "2.4.1",
    "@react-navigation/bottom-tabs": "6.3.1",
    "@react-navigation/native": "6.0.10",
    "@react-navigation/native-stack": "6.6.2",
    "appcenter": "^4.4.4",
    "appcenter-analytics": "^4.4.4",
    "appcenter-crashes": "^4.4.4",
    "axios": "0.27.2",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-code-push": "^7.0.4",
    "react-native-config": "^1.4.6",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localization": "^2.3.1",
    "react-native-localize": "^2.2.2",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "4.2.5",
    "react-native-screens": "3.13.1",
    "react-native-skeleton-placeholder": "5.0.0",
    "react-native-svg-transformer": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.17.12",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^9.1.0",
    "@types/jest": "^28.1.3",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-dotenv-import": "^2.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-native-dotenv": "^3.3.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^28.0.4",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
